{
  "name": "vite-plugin-with-nitro",
  "type": "module",
  "version": "0.0.0-beta.6",
  "packageManager": "pnpm@9.12.1",
  "description": "A Vite plugin for adding a nitro API server, forked from @analogjs/vite-plugin-nitro",
  "author": "Ou Rongxing <orongxing@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/ouronxing/vite-plugin-with-nitro",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ouronxing/vite-plugin-with-nitro"
  },
  "bugs": {
    "url": "https://github.com/ouronxing/vite-plugin-with-nitro/issues"
  },
  "keywords": [
    "vite",
    "plugin-vite",
    "nitro"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "unbuild --stub",
    "build": "unbuild",
    "test": "vitest",
    "release": "bumpp && pnpm publish",
    "lint": "eslint ."
  },
  "peerDependencies": {
    "vite": "^5.0.0"
  },
  "dependencies": {
    "consola": "^3.2.3",
    "esbuild": "^0.24.0",
    "fast-glob": "^3.3.2",
    "front-matter": "^4.0.2",
    "h3": "^1.13.0",
    "nitropack": "^2.9.7",
    "xmlbuilder2": "^3.1.1"
  },
  "devDependencies": {
    "@ourongxing/eslint-config": "3.2.3-beta.3",
    "@types/node": "^22.7.5",
    "bumpp": "^9.7.1",
    "eslint": "^9.12.0",
    "typescript": "^5.6.3",
    "unbuild": "^2.0.0",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.1.3"
  }
}
